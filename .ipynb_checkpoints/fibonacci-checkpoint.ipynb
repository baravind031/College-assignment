    "def fibonacci(num):\n",
    "    if num <= 1:\n",
    "        return num\n",
    "    else :\n",
    "        return(fibonacci(num-1)+fibonacci(num-2))\n",
    "\n",
    "inpu_t = int(input(\"Enter input\"))\n",
    "\n",
    "for i in range(inpu_t):\n",
    "    print(fibonacci(i), end=\" \")"
   ]
