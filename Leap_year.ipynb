    "t_input = int(input(\"Enter year\"))\n",
    "\n",
    "def leap(y):\n",
    "    if (y%4 == 0 and y %4 != 0) or (y%4 == 0):\n",
    "        return \"leap year\"\n",
    "    else: \n",
    "        return \"Not a Leap year\"\n",
    "    \n",
    "print(leap(t_input))"
